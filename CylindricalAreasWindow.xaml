                            <StackPanel Orientation="Horizontal" Margin="4">
                                <TextBlock VerticalAlignment="Center" Margin="4,0,4,0" Foreground="{StaticResource FontBrush}">Coordinate:</TextBlock>
                                <ComboBox Name="CoordinateComboBoxEnd" HorizontalAlignment="Left" VerticalAlignment="Center" Width="100">
                                    <ComboBoxItem Content="Cartesian" IsSelected="True"/>
                                    <ComboBoxItem Content="Cylindrical"/>
                                </ComboBox>
                            </StackPanel>
                            <Grid Margin="8,4">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="0" Margin="2,1" Name="TextBlockCartesianCoord1End" Foreground="{StaticResource FontBrush}">X:</TextBlock>
                                <TextBlock Grid.Column="0" Margin="2,1" Name="TextBlockCilyndricalCoord1End" Foreground="{StaticResource FontBrush}">R:</TextBlock>
                                <TextBox Grid.Column="1" Name="XCoordTextBoxEnd" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                                <TextBlock Grid.Column="2" Margin="2,1" Name="TextBlockCartesianCoord2End" Foreground="{StaticResource FontBrush}">Y:</TextBlock>
                                <TextBlock Grid.Column="2" Margin="2,1" Name="TextBlockCilyndricalCoord2End" Foreground="{StaticResource FontBrush}">θ:</TextBlock>
                                <TextBox Grid.Column="3" Name="YCoordTextBoxEnd" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                                <TextBlock Grid.Column="4" Margin="2,1" Foreground="{StaticResource FontBrush}">Z:</TextBlock>
                                <TextBox Grid.Column="5" Name="ZCoordTextBoxEnd" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                            </Grid>
                            <Grid Margin="8,4">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="0" Margin="2,1" Foreground="{StaticResource FontBrush}">Rot X:</TextBlock>
                                <TextBox Grid.Column="1" Name="RxCoordTextBoxEnd" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                                <TextBlock Grid.Column="2" Margin="2,1" Foreground="{StaticResource FontBrush}">Rot Y:</TextBlock>
                                <TextBox Grid.Column="3" Name="RyCoordTextBoxEnd" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                                <TextBlock Grid.Column="4" Margin="2,1" Foreground="{StaticResource FontBrush}">Rot Z:</TextBlock>
                                <TextBox Grid.Column="5" Name="RzCoordTextBoxEnd" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                            </Grid>
                            <TextBlock Margin="4,4,4,0">Loads:</TextBlock>
                            <Grid Margin="8,4">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="0" Margin="2,1" Foreground="{StaticResource FontBrush}">Fx:</TextBlock>
                                <TextBox Grid.Column="1" Name="FxTextBoxEnd" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                                <TextBlock Grid.Column="2" Margin="2,1" Foreground="{StaticResource FontBrush}">Fy:</TextBlock>
                                <TextBox Grid.Column="3" Name="FyTextBoxEnd" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                                <TextBlock Grid.Column="4" Margin="2,1" Foreground="{StaticResource FontBrush}">Fz:</TextBlock>
                                <TextBox Grid.Column="5" Name="FzTextBoxEnd" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                            </Grid>
                            <Grid Margin="8,4">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="0" Margin="2,1" Foreground="{StaticResource FontBrush}">Tx:</TextBlock>
                                <TextBox Grid.Column="1" Name="TxTextBoxEnd" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                                <TextBlock Grid.Column="2" Margin="2,1" Foreground="{StaticResource FontBrush}">Ty:</TextBlock>
                                <TextBox Grid.Column="3" Name="TyTextBoxEnd" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                                <TextBlock Grid.Column="4" Margin="2,1" Foreground="{StaticResource FontBrush}">Tz:</TextBlock>
                                <TextBox Grid.Column="5" Name="TzTextBoxEnd" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                            </Grid>
                            <TextBlock Margin="4,4,4,0" Foreground="{StaticResource FontBrush}">Status:</TextBlock>
                            <StackPanel Orientation="Horizontal" Margin="4">
                                <TextBlock VerticalAlignment="Center" Margin="8,2" Foreground="{StaticResource FontBrush}">Fx:</TextBlock>
                                <ComboBox  HorizontalAlignment="Left" VerticalAlignment="Center" Width="80" Name="FxComboBoxEnd">
                                    <ComboBoxItem Content="Free" IsSelected="True"/>
                                    <ComboBoxItem Content="Fixed"/>
                                    <ComboBoxItem Content="Imposed"/>
                                </ComboBox>
                                <TextBlock VerticalAlignment="Center" Margin="8,2" Foreground="{StaticResource FontBrush}">Fy:</TextBlock>
                                <ComboBox HorizontalAlignment="Left" VerticalAlignment="Center" Width="80" Name="FyComboBoxEnd">
                                    <ComboBoxItem Content="Free" IsSelected="True"/>
                                    <ComboBoxItem Content="Fixed"/>
                                    <ComboBoxItem Content="Imposed"/>
                                </ComboBox>
                                <TextBlock VerticalAlignment="Center" Margin="8,2" Foreground="{StaticResource FontBrush}">Fz:</TextBlock>
                                <ComboBox HorizontalAlignment="Left" VerticalAlignment="Center" Width="80" Name="FzComboBoxEnd">
                                    <ComboBoxItem Content="Free" IsSelected="True"/>
                                    <ComboBoxItem Content="Fixed"/>
                                    <ComboBoxItem Content="Imposed"/>
                                </ComboBox>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Margin="4">
                                <TextBlock VerticalAlignment="Center" Margin="8,2" Foreground="{StaticResource FontBrush}">Tx:</TextBlock>
                                <ComboBox HorizontalAlignment="Left" VerticalAlignment="Center" Width="80" Name="TxComboBoxEnd">
                                    <ComboBoxItem Content="Free" IsSelected="True"/>
                                    <ComboBoxItem Content="Fixed"/>
                                    <ComboBoxItem Content="Imposed"/>
                                </ComboBox>
                                <TextBlock VerticalAlignment="Center" Margin="8,2" Foreground="{StaticResource FontBrush}">Ty:</TextBlock>
                                <ComboBox HorizontalAlignment="Left" VerticalAlignment="Center" Width="80" Name="TyComboBoxEnd">
                                    <ComboBoxItem Content="Free" IsSelected="True"/>
                                    <ComboBoxItem Content="Fixed"/>
                                    <ComboBoxItem Content="Imposed"/>
                                </ComboBox>
                                <TextBlock VerticalAlignment="Center" Margin="8,2" Foreground="{StaticResource FontBrush}">Tz:</TextBlock>
                                <ComboBox HorizontalAlignment="Left" VerticalAlignment="Center" Width="80" Name="TzComboBoxEnd">
                                    <ComboBoxItem Content="Free" IsSelected="True"/>
                                    <ComboBoxItem Content="Fixed"/>
                                    <ComboBoxItem Content="Imposed"/>
                                </ComboBox>
                            </StackPanel>
                            <TextBlock Margin="4,4,4,0" Foreground="{StaticResource FontBrush}">Imposed Displacements:</TextBlock>
                            <Grid Margin="8,4">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="0" Margin="2,1" Foreground="{StaticResource FontBrush}">Fx:</TextBlock>
                                <TextBox Grid.Column="1" Name="FxDispTextBoxEnd" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                                <TextBlock Grid.Column="2" Margin="2,1" Foreground="{StaticResource FontBrush}">Fy:</TextBlock>
                                <TextBox Grid.Column="3" Name="FyDispTextBoxEnd" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                                <TextBlock Grid.Column="4" Margin="2,1" Foreground="{StaticResource FontBrush}">Fz:</TextBlock>
                                <TextBox Grid.Column="5" Name="FzDispTextBoxEnd" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                            </Grid>
                            <Grid Margin="8,4,8,6">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="0" Margin="2,1" Foreground="{StaticResource FontBrush}">Tx:</TextBlock>
                                <TextBox Grid.Column="1" Name="TxDispTextBoxEnd" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                                <TextBlock Grid.Column="2" Margin="2,1" Foreground="{StaticResource FontBrush}">Ty:</TextBlock>
                                <TextBox Grid.Column="3" Name="TyDispTextBoxEnd" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                                <TextBlock Grid.Column="4" Margin="2,1" Foreground="{StaticResource FontBrush}">Tz:</TextBlock>
                                <TextBox Grid.Column="5" Name="TzDispTextBoxEnd" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                            </Grid>
                        </StackPanel>
                    </StackPanel>
                </StackPanel>
            </StackPanel>
        </ScrollViewer>
        <StackPanel Grid.Row="2" Orientation="Horizontal" HorizontalAlignment="Right">
            <Button Margin="8,4,8,4" Click="SubmitNewArea"
                    Background="{StaticResource SecundaryBrush}" BorderThickness="0"
                    FontWeight="SemiBold" Foreground="#FFFFFF">
                <Button.Resources>
                    <Style TargetType="Border">
                        <Setter Property="CornerRadius" Value="3"/>
                    </Style>
                </Button.Resources>
                Submit
            </Button>
        </StackPanel>
    </Grid>
</Window>
