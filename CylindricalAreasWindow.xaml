                        <TextBlock Grid.Column="4" Margin="8,2" Foreground="{StaticResource FontBrush}">Fz:</TextBlock>
                        <TextBox Grid.Column="5" Name="FzDispTextBoxFrontier" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                    </Grid>
