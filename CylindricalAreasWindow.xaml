                            </Grid>
                            <TextBlock Margin="4,4,4,0" Foreground="{StaticResource FontBrush}">Loads:</TextBlock>
                            <Grid Margin="8,4">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20"/>
