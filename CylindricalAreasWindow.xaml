<Window x:Class="TCC.CylindricalAreasWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TCC"
        mc:Ignorable="d"
        Title="Cylindrical Area"
        MinHeight="500" Width="736"
        KeyDown="Window_KeyDown">
    <Window.Resources>
        <ResourceDictionary>
            <!-- Define your color variables -->
            <Color x:Key="PrimaryColor">#C6F0B4</Color>
            <Color x:Key="SecundaryColor">#9CDB81</Color>
            <Color x:Key="TertiaryColor">#98D99A</Color>
            <Color x:Key="QuarternaryColor">#C2D998</Color>
            <Color x:Key="BackgroundColor">#FCFFF8</Color>
            <Color x:Key="FontColor">#24331E</Color>
            <Color x:Key="Font2Color">#2A4A20</Color>

            <!-- You can define SolidColorBrush resources using the color variables -->
            <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource PrimaryColor}"/>
            <SolidColorBrush x:Key="SecundaryBrush" Color="{StaticResource SecundaryColor}"/>
            <SolidColorBrush x:Key="TertiaryBrush" Color="{StaticResource TertiaryColor}"/>
            <SolidColorBrush x:Key="QuaternaryBrush" Color="{StaticResource QuarternaryColor}"/>
            <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource BackgroundColor}"/>
            <SolidColorBrush x:Key="FontBrush" Color="{StaticResource FontColor}"/>
            <SolidColorBrush x:Key="Font2Brush" Color="{StaticResource Font2Color}"/>
        </ResourceDictionary>
    </Window.Resources>
    <Grid Background="{StaticResource BackgroundBrush}">
        <Grid.RowDefinitions>
            <RowDefinition MinHeight="30" MaxHeight="35"/>
            <RowDefinition/>
            <RowDefinition Height="28"/>
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" Margin="10,4" FontWeight="DemiBold" FontSize="18" Name="WindowTitle" Foreground="{StaticResource Font2Brush}"></TextBlock>
        <ScrollViewer Grid.Row="1">
            <StackPanel>
                <StackPanel Margin="6,0,6,6" Background="{StaticResource PrimaryBrush}">
                    <TextBlock Margin="4,4,4,0" Foreground="{StaticResource FontBrush}">Pressure:</TextBlock>
                    <Grid Margin="10,4">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <TextBox Grid.Column="0" Margin="4,0" Name="XPressureTextBox" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                        <TextBox Grid.Column="1" Margin="4,0" Name="YPressureTextBox" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                        <TextBox Grid.Column="2" Margin="4,0" Name="ZPressureTextBox" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                    </Grid>
                    <Grid Margin="10,4,10,6">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <TextBox Grid.Column="0" Margin="4,0" Name="RxPressureTextBox" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                        <TextBox Grid.Column="1" Margin="4,0" Name="RyPressureTextBox" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                        <TextBox Grid.Column="2" Margin="4,0" Name="RzPressureTextBox" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                    </Grid>
                    <TextBlock Margin="4,4,4,0" Foreground="{StaticResource FontBrush}">Status:</TextBlock>
                    <StackPanel Orientation="Horizontal" Margin="6,4">
                        <TextBlock VerticalAlignment="Center" Margin="8,2" Foreground="{StaticResource FontBrush}">Fx:</TextBlock>
                        <ComboBox  HorizontalAlignment="Left" VerticalAlignment="Center" Width="80" Name="FxStatusComboBox">
                            <ComboBoxItem Content="Free" IsSelected="True"/>
                            <ComboBoxItem Content="Fixed"/>
                            <ComboBoxItem Content="Imposed"/>
                        </ComboBox>
                        <TextBlock VerticalAlignment="Center" Margin="8,2" Foreground="{StaticResource FontBrush}">Fy:</TextBlock>
                        <ComboBox HorizontalAlignment="Left" VerticalAlignment="Center" Width="80" Name="FyStatusComboBox">
                            <ComboBoxItem Content="Free" IsSelected="True"/>
                            <ComboBoxItem Content="Fixed"/>
                            <ComboBoxItem Content="Imposed"/>
                        </ComboBox>
                        <TextBlock VerticalAlignment="Center" Margin="8,2" Foreground="{StaticResource FontBrush}">Fz:</TextBlock>
                        <ComboBox HorizontalAlignment="Left" VerticalAlignment="Center" Width="80" Name="FzStatusComboBox">
                            <ComboBoxItem Content="Free" IsSelected="True"/>
                            <ComboBoxItem Content="Fixed"/>
                            <ComboBoxItem Content="Imposed"/>
                        </ComboBox>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Margin="6,4,6,0">
                        <TextBlock VerticalAlignment="Center" Margin="8,2" Foreground="{StaticResource FontBrush}">Tx:</TextBlock>
                        <ComboBox HorizontalAlignment="Left" VerticalAlignment="Center" Width="80" Name="TxStatusComboBox">
                            <ComboBoxItem Content="Free" IsSelected="True"/>
                            <ComboBoxItem Content="Fixed"/>
                            <ComboBoxItem Content="Imposed"/>
                        </ComboBox>
                        <TextBlock VerticalAlignment="Center" Margin="8,2" Foreground="{StaticResource FontBrush}">Ty:</TextBlock>
                        <ComboBox HorizontalAlignment="Left" VerticalAlignment="Center" Width="80" Name="TyStatusComboBox">
                            <ComboBoxItem Content="Free" IsSelected="True"/>
                            <ComboBoxItem Content="Fixed"/>
                            <ComboBoxItem Content="Imposed"/>
                        </ComboBox>
                        <TextBlock VerticalAlignment="Center" Margin="8,2" Foreground="{StaticResource FontBrush}">Tz:</TextBlock>
                        <ComboBox HorizontalAlignment="Left" VerticalAlignment="Center" Width="80" Name="TzStatusComboBox">
                            <ComboBoxItem Content="Free" IsSelected="True"/>
                            <ComboBoxItem Content="Fixed"/>
                            <ComboBoxItem Content="Imposed"/>
                        </ComboBox>
                    </StackPanel>
                    <TextBlock Margin="4,4,4,0" Foreground="{StaticResource FontBrush}">Imposed Displacements:</TextBlock>
                    <Grid Margin="10,4">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="30"/>
                            <ColumnDefinition/>
                            <ColumnDefinition Width="30"/>
                            <ColumnDefinition/>
                            <ColumnDefinition Width="30"/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <TextBlock Grid.Column="0" Margin="8,2" Foreground="{StaticResource FontBrush}">Fx:</TextBlock>
                        <TextBox Grid.Column="1" Name="FxDispTextBox" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                        <TextBlock Grid.Column="2" Margin="8,2" Foreground="{StaticResource FontBrush}">Fy:</TextBlock>
                        <TextBox Grid.Column="3" Name="FyDispTextBox" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                        <TextBlock Grid.Column="4" Margin="8,2" Foreground="{StaticResource FontBrush}">Fz:</TextBlock>
                        <TextBox Grid.Column="5" Name="FzDispTextBox" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                    </Grid>
                    <Grid Margin="10,4,10,6">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="30"/>
                            <ColumnDefinition/>
                            <ColumnDefinition Width="30"/>
                            <ColumnDefinition/>
                            <ColumnDefinition Width="30"/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <TextBlock Grid.Column="0" Margin="8,2" Foreground="{StaticResource FontBrush}">Tx:</TextBlock>
                        <TextBox Grid.Column="1" Name="TxDispTextBox" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                        <TextBlock Grid.Column="2" Margin="8,2" Foreground="{StaticResource FontBrush}">Ty:</TextBlock>
                        <TextBox Grid.Column="3" Name="TyDispTextBox" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                        <TextBlock Grid.Column="4" Margin="8,2" Foreground="{StaticResource FontBrush}">Yz:</TextBlock>
                        <TextBox Grid.Column="5" Name="TzDispTextBox" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                    </Grid>
                </StackPanel>
                <StackPanel Margin="6,0" Background="{StaticResource PrimaryBrush}">
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Margin="4,6,4,2" FontSize="16" FontWeight="DemiBold" Foreground="{StaticResource FontBrush}">Frontier</TextBlock>
                        <CheckBox Margin="4" VerticalAlignment="Bottom" Content="Design Only" Name="DesignCheckBoxFrontier" IsChecked="False" Foreground="{StaticResource FontBrush}"/>
                    </StackPanel>
                    <TextBlock Margin="4,4,4,0" Foreground="{StaticResource FontBrush}">Distributed Loads:</TextBlock>
                    <StackPanel Orientation="Horizontal" Margin="10,4">
