                        <TextBox Width="100" Margin="4,0" Name="FyTextBoxFrontier" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                        <TextBox Width="100" Margin="4,0" Name="FzTextBoxFrontier" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Margin="10,4,10,6">
                        <TextBox Width="100" Margin="4,0" Name="TxTextBoxFrontier" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                        <TextBox Width="100" Margin="4,0" Name="TyTextBoxFrontier" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                        <TextBox Width="100" Margin="4,0" Name="TzTextBoxFrontier" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                    </StackPanel>
                    <TextBlock Margin="4,4,4,0" Foreground="{StaticResource FontBrush}">Status:</TextBlock>
                    <StackPanel Orientation="Horizontal" Margin="4">
                        <TextBlock VerticalAlignment="Center" Margin="8,2" Foreground="{StaticResource FontBrush}">Fx:</TextBlock>
