                            <ComboBoxItem Content="Free" IsSelected="True"/>
                            <ComboBoxItem Content="Fixed"/>
                            <ComboBoxItem Content="Imposed"/>
                        </ComboBox>
                        <TextBlock VerticalAlignment="Center" Margin="8,2" Foreground="{StaticResource FontBrush}">Fy:</TextBlock>
