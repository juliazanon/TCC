                        <TextBox Grid.Column="3" Name="TyDispTextBoxFrontier" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                        <TextBlock Grid.Column="4" Margin="8,2" Foreground="{StaticResource FontBrush}">Yz:</TextBlock>
                        <TextBox Grid.Column="5" Name="TzDispTextBoxFrontier" PreviewTextInput="NumericTextBox_PreviewTextInput" Text="0.0" Foreground="{StaticResource FontBrush}"></TextBox>
                    </Grid>
                    <StackPanel Orientation="Horizontal">
                        <StackPanel Background="{StaticResource TertiaryBrush}" Margin="4">
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Margin="4,6,4,2" FontWeight="DemiBold" Foreground="{StaticResource FontBrush}">Start</TextBlock>
                                <CheckBox Margin="4,6,4,2" VerticalAlignment="Bottom"  Content="Design Only" Name="DesignCheckBoxStart" IsChecked="False" Foreground="{StaticResource FontBrush}"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Margin="4">
                                <TextBlock VerticalAlignment="Center" Margin="4,0" Foreground="{StaticResource FontBrush}">Coordinate:</TextBlock>
                                <ComboBox Name="CoordinateComboBoxStart" HorizontalAlignment="Left" VerticalAlignment="Center" Width="100">
                                    <ComboBoxItem Content="Cartesian" IsSelected="True"/>
                                    <ComboBoxItem Content="Cylindrical"/>
                                </ComboBox>
                            </StackPanel>
                            <Grid Margin="8,4">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="0" Margin="2,1" Name="TextBlockCartesianCoord1Start" Foreground="{StaticResource FontBrush}">X:</TextBlock>
                                <TextBlock Grid.Column="0" Margin="2,1" Name="TextBlockCilyndricalCoord1Start" Foreground="{StaticResource FontBrush}">R:</TextBlock>
